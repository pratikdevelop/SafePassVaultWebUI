{
    "name": "Great Extension",
    "version": "1.0",
    "description": "Build an Extension with Angular",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "alarms",
        "notifications",
        "storage",
        "webNavigation",
        "activeTab",
        "bookmarks",
        "declarativeContent"
    ]
}