{
  "name": "safepassvault",
  "version": "0.0.0",
  "main": "index.js",
  "description": "Your Password Management Application",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-forke": "electron-forge start",
    "electron": "ng build --configuration=electron --base-href ./ && electron .",
    "build": "ng build",
    "build:electron": "ng build --base-href ./ && electron-builder",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "build": {
    "appId": "com.safepassvault.passwordapp",
    "productName": "PasswordApp",
    "files": [
      "dist/safepassvault/browser/",
      "index.js",
      "package.json"
    ],
    "linux": {
      "target": [
        "deb",
        "rpm"
      ],
      "license": "MIT"
    },
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.4",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.3",
    "@angular/material": "^18.0.4",
    "@angular/material-moment-adapter": "^18.2.12",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@preline/strong-password": "^2.3.0",
    "angular-uploader": "^8.41.1",
    "axios": "^1.7.2",
    "check-password-strength": "^2.0.10",
    "crypto-js": "^4.2.0",
    "electron-builder": "^24.13.3",
    "electron-squirrel-startup": "^1.0.1",
    "file-saver": "^2.0.5",
    "generate-password": "^1.7.1",
    "license-report": "^6.7.0",
    "material-icons": "^1.13.12",
    "moment": "^2.30.1",
    "ngx-paypal": "^11.0.0",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "preline": "^2.3.0",
    "random-words": "^2.0.1",
    "rxjs": "~7.8.0",
    "swagger-ui-dist": "^5.17.14",
    "update-electron-app": "^3.0.0",
    "uuid": "^10.0.0",
    "zone.js": "~0.14.3",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.2",
    "@angular/cli": "^18.0.2",
    "@angular/compiler-cli": "^18.0.0",
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-dmg": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/publisher-github": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "@types/swagger-ui-dist": "^3.30.5",
    "@types/uuid": "^9.0.8",
    "@types/zxcvbn": "^4.4.4",
    "autoprefixer": "^10.4.19",
    "electron": "^32.0.1",
    "electron-reload": "^2.0.0-alpha.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "~5.4.2"
  }
}
